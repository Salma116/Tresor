--ui

function draw_ui()
	camera()
	spr(1,2,2)
	outline("X"..p.keys,10,2)	
	spr(2,21,2)
	outline("X"..p.champi,30,2)
end



function outline(text,x,y)
	print(text,x-1,y,0)
	print(text,x+1,y,0)
	print(text,x,y-1,0)
	print(text,x,y+1,0)
	print(text,x,y,7)
end


	

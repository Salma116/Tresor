--message
function init_msg()
	messages={}
end

function create_msg(name,...)
	msg_title=name
	messages={...}
end

function update_msg()
	if btnp(❎) then 
		deli(messages,1)
	end
end

function draw_msg()
	if messages[1] then
	local y=100
	local x=0
	if p.y%16>9 then 
		y=10
	end
 if p.x>16 and p.x<31 then
	x=128
	end
	rectfill(x+6+1,y+1,
		x+6+#msg_title*4+2,y+6+1,0)
	rectfill(x+2+1,y+9+1,x+125+1,y+20+1,0)
	
	rectfill(x+6,y,
		x+6+#msg_title*4,y+6,14)
	rectfill(x+2,y+9,x+125,y+20,2)
	print(msg_title,x+7,y+1,7)
	print(messages[1],x+4,y+10,7)
	end
end
	


--player
function create_player()
p={x=6,y=6,sprite=19,keys=0,
champi=0}
end

function player_mouvment()
  newx=p.x
  newy=p.y
		if (btnp(➡️))	newx+=1 
		if (btnp(⬅️))	newx-=1	
		if (btnp(⬇️))	newy+=1
		if (btnp(⬆️))	newy-=1
		
		
	 --⬆️maintenir appuyer+❎
	 --if (btn(❎)) then
		interact(newx,newy)
		--end
		
		
	 if not check_flag(0,newx,newy)
	then 
 		p.x=newx
 		p.y=newy
 	end
end

function interact(x,y)
	if check_flag(1,x,y) 
		then pick_up_key(x,y)
		
	elseif check_flag(2,x,y)
		and p.keys>0 then
		pacman_move(x,y)
		sfx(2)
		passage=true
		
	elseif check_flag(3,x,y)
		and p.keys>=3 then
		chloe_move(x,y)
		sfx(2)
		marecage=true
		
	elseif check_flag(5,x,y)
		then pick_up_champi(x,y)
		poisson_rouge()
	end
	
		--message salle 1
		if x==15 and y==7
		and not passage then
		 create_msg("mr pole emploi","j'ai faim",
	"ventre vide = pas de formation")
  end
  --message salle 2
  if x==30 and y==7
  and not marecage then
		 create_msg("chloe","bienvenue a ada",
	"file au tableau")
  end
  if x==21 and y==0 then
		 create_msg("tableau","pour avoir un stage",
	"il te faudra 3 outils")
  end
  if x==24 and y==0 then
		 create_msg("tableau","les anciens ont laisse",
	"des affaires dans les poubelles")
  end
  if x==25 and y==0 then
		 create_msg("tableau","cherche dans la doc")
  end
  if x==28 and y==0 then
		 create_msg("tableau","verifie les tables")
  end
end

function draw_player()
spr(p.sprite,p.x*8,p.y*8)
end


	

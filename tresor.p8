function _init()
	create_player()
	init_msg()
//	create_msg("mr pole emploi","j'ai faim",
//	"ventre vide = pas de formation")
end

function _update()
	if not messages[1] then
		player_mouvment()
	end
	update_camera()
	update_msg()
end		

function _draw()
	cls()
	draw_map()
	draw_player()
	draw_msg()
	draw_ui()
end

